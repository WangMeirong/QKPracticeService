<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.qk.practice.dao.IAnwserDao">	<resultMap type="Anwser" id="AnwserResultMap">		<id property="anwserId" column="anwser_id"/>		<result property="anwser" column="anwser"/>		<result property="attachment" column="attachment"/>		<result property="isDelete" column="is_delete"/>		<result property="createdTime" column="created_time"/>		<result property="lastModifiedBy" column="last_modified_by"/>		<result property="lastModifiedTime" column="last_modified_time"/>	</resultMap>		<sql id="Anwser_Column_List" >		anwser_id,anwser,attachment,is_delete,created_time,last_modified_by,last_modified_time	</sql>		<select id="getAnwserById" resultMap="AnwserResultMap" parameterType="java.lang.String" >	    select <include refid="Anwser_Column_List"></include>	    from Anwser	    where is_delete = 'N'	    and anwser_id = #{anwser_id,jdbcType=VARCHAR}	 </select></mapper>