<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.qk.practice.dao.ITagDao">	<resultMap type="Tag" id="TagResultMap">		<id property="tagId" column="tag_id"/>		<result property="tag" column="tag"/>		<result property="isDelete" column="is_delete"/>		<result property="createdTime" column="created_time"/>		<result property="lastModifiedBy" column="last_modified_by"/>		<result property="lastModifiedTime" column="last_modified_time"/>	</resultMap>	<sql id="Tag_Column_List">		tag_id,tag,is_delete,created_time,last_modified_by,last_modified_time	</sql>		 	<select id="getTagByPracticeId" resultMap="TagResultMap" parameterType="java.lang.String" >	    select tag.tag_id,tag.tag,tag.created_time,tag.last_modified_by,tag.last_modified_time	    from tag tag, practice_tag pt	    where pt.tag_id = tag.tag_id and tag.is_delete = 'N'	    and pt.practice_id = #{practice_id} 	</select>		<select id="getAllTags" resultMap="TagResultMap" >	    select <include refid="Tag_Column_List"></include>	    from tag where is_delete = 'N'	</select></mapper>