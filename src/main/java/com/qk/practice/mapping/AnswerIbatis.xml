<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.qk.practice.dao.IAnswerDao">	<resultMap type="Answer" id="AnswerResultMap">		<id property="answerId" column="answer_id"/>		<result property="answer" column="answer"/>		<result property="attachment" column="attachment"/>		<result property="isDelete" column="is_delete"/>		<result property="createdTime" column="created_time"/>		<result property="lastModifiedBy" column="last_modified_by"/>		<result property="lastModifiedTime" column="last_modified_time"/>	</resultMap>		<sql id="Answer_Column_List" >		answer_id,answer,attachment,is_delete,created_time,last_modified_by,last_modified_time	</sql>		<select id="getAnswerById" resultMap="AnswerResultMap" parameterType="java.lang.String" >	    select <include refid="Answer_Column_List"></include>	    from answer	    where is_delete = 'N'	    and answer_id = #{answer_id,jdbcType=VARCHAR}	 </select>	 	 <insert id="insertAnswer" parameterType="Answer">	 	insert into answer	 	<trim prefix="(" suffix=")" suffixOverrides="," >	 		<include refid="Answer_Column_List"></include>	 	</trim>	 	values (#{answerId},#{answer},#{attachment},'N',unix_timestamp() * 1000,#{lastModifiedBy},unix_timestamp() * 1000)	 </insert>	 	 <update id="deleteAnswerById" parameterType="Answer" >	 	update answer set 	 	is_delete = 'Y', 	 	last_modified_by = #{lastModifiedBy},	 	last_modified_time = unix_timestamp() * 1000	 	where answer_id = #{answerId}	 </update>	 	 <update id="updateAnswerById" parameterType="Answer">	 	update answer set	 	answer = #{answer},	 	attachment = #{attachment},	 	last_modified_by = #{lastModifiedBy},	 	last_modified_time = unix_timestamp() * 1000	 	where answer_id =#{answerId}	 </update></mapper>